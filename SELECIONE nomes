SELECT name FROM world
  WHERE name LIKE 'Y%'

SELECT name FROM world
  WHERE name LIKE '%Y'

SELECT name FROM world
  WHERE name LIKE '%X%'

SELECT name FROM world
  WHERE name LIKE '%T%'

SELECT name FROM world
  WHERE name LIKE 'c%ia'

SELECT name FROM world
  WHERE name LIKE '%oo%'

SELECT name FROM world
  WHERE name LIKE '%a%a%a%'

SELECT name FROM world
 WHERE name LIKE '_T%'
ORDER BY name


SELECT name FROM world
 WHERE name LIKE '%o__o%'

SELECT name FROM world
 WHERE length(name) = 4;

SELECT name
FROM world
WHERE name = capital;

SELECT name
FROM world
WHERE capital = CONCAT(name, ' City');



